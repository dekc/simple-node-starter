{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Launch program",
        "program": "${file}",
        "runtimeExecutable": null,
        "args": [
          "-r", "babel-register"
        ],
        "env": {
          "NODE_ENV": "development"
        },
        "cwd": "${workspaceRoot}",
        "stopOnEntry": false,
        "sourceMaps": true,
        "protocol": "inspector"
      },
    ]
}
